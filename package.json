{
  "name": "@crystal-ball/node-service-prototype",
  "version": "0.0.0",
  "description": "Prototypical Node.js service",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/node-service-prototype.git"
  },
  "keywords": [
    "prototype",
    "nodejs",
    "service"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "start:dev": "nodemon --inspect ./dist/index.js",
    "watch": "babel src --out-dir dist --extensions .ts --watch",
    "test": "npm run format",
    "lint": "NODE_ENV=test eslint --format=pretty src/**/*.ts",
    "format": "prettier --write src/*.js"
  },
  "dependencies": {
    "express": "4.17.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/preset-typescript": "7.3.3",
    "@crystal-ball/commitizen-base": "1.0.0",
    "@crystal-ball/prettier-base": "1.0.0",
    "@types/express": "4.16.1",
    "@types/node": "12.0.2",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "eslint-config-eloquence": "11.2.0",
    "husky": "2.3.0",
    "nodemon": "1.19.0",
    "typescript": "3.4.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "prettier": "@crystal-ball/prettier-base"
}
